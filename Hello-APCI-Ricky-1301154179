a. Ubah isi file Hello-APCI-Asprak-NIM
            Program Hello; // ubah menjadi Program APCI-NIM
            Uses Crt;
            Begin
                ClrScr;
                Writeln(‘Hello’);//untuk mengeluarkan string ‘Hello’ di layar
                Writeln(‘1301154179’); //Ubah untuk menampilkan NIM Anda
                Writeln(‘Andynata tamtama suseno’); //Ubah untuk menampilkan Nama Anda
                Writeln(‘IF 39-11’); //Ubah untuk menampilkan Kelas Anda
                Writeln(‘Ricky’); //Ubah untuk menampilkan Nama Asprak Anda
                Writeln(‘1301154123’); //Ubah untuk menampilkan NIM teman anda
                Writeln(‘Modul 6’); //Ubah untuk menampilkan string ‘Modul 6
            End.
